@{
	ViewData["Title"] = @Model.Get("QualifiedName", "My Project");
}

<!DOCTYPE html>
<html lang="en">

<head>
    <title>@ViewData["Title"]</title>
    
    <script src="@Model.String("PathPrefix", string.Empty)js/jquery-2.1.4.min.js"></script>
    
    <link rel="stylesheet" target="blank" href="https://fonts.googleapis.com/css?family=Roboto:300,300italic,700,700italic">
    <link href="@Model.String("PathPrefix", string.Empty)css/milligram.min.css" rel="stylesheet" />
    <link href="@Model.String("PathPrefix", string.Empty)css/font-awesome.min.css" rel="stylesheet" />
    <link href="@Model.String("PathPrefix", string.Empty)css/site.css" rel="stylesheet" />
</head>

<body>
    <main>
        <div class="container">
            <div class="row">
                <div class="column">
                    @Html.Partial("Sidebar\\_NamespaceTree")
                </div>
                <div class="column column-75">
                    @RenderBody()
                </div>
            </div>
        </div>
    </main>
</body>

</html>