@using System.Text.RegularExpressions;

@{
	Layout = @"_Layout.cshtml";
}
        
<h2>@Model.String("FullName") <small>@Model.String("SpecificKind")</small></h2>

<p>@Regex.Replace(Model.String("Summary"), "<.*?>", string.Empty)</p>

@Html.Partial("Section\\_Syntax")
@Html.Partial("Section\\_EnumMember")
@Html.Partial("Section\\_Properties")
@Html.Partial("Section\\_Methods")

@RenderBody()